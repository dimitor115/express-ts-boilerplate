{
    "swagger": {
        "outputDirectory": "./dist",
        "entryFile": "./src/server.ts",
        "basePath": "/api/v1",
        "middleware": "express",
        "controllerPathGlobs": [
            "./src/api/*"
        ]
    },
    "routes": {
        "basePath": "/api/v1",
        "entryFile": "./src/server.ts",
        "routesDir": "./src/api/_auto"
    },
    "ignore": [
      "**/node_modules/**",
      "./src/api/_auto/*"
    ]
}